<% include partials/header %>

<h1><%= campground.name %></h1>

<img src = "<%= campground.image %>">

<p><%= campground.description %></p>

<% campground.comments.forEach(function(commentData){ %>
    
    <p> 
        <strong><%= commentData.author %></strong>
        - <%= commentData.text %> 
    </p>
    
<% }); %>

<% include partials/footer %>